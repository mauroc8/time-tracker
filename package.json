{
  "name": "time-tracker",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "layout.css": "^1.0.0"
  },
  "scripts": {
    "start": "parcel src/index.html",
    "build": "rm -rf docs && parcel build src/index.html --public-url=./ --out-dir docs --detailed-report",
    "typecheck": "tsc -p . -w"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "parcel": "^2.0.0-beta.2",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-static-files-copy": "^2.5.1",
    "typescript": "^4.2.2"
  }
}
